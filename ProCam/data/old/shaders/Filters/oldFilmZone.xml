<filter name="Blur Zone">
    <description>Blur Zone</description>
    <vertex>default1.vert</vertex>        
    <fragment>oldFilmZone.frag</fragment>    
    <textures input="2" output="1">
        <intexture type="sampler2d" name="Sample0" offset="offset" label="Input texture">0</intexture> 
        <intexture type="sampler2d" name="mask" offset="offset" label="mask texture">1</intexture> 
    </textures>
    <parameter type="float" name="SepiaValue" label="pixel size">0.7</parameter>
    <parameter type="float" name="NoiseValue" label="pixel size">0.3</parameter>
    <parameter type="float" name="ScratchValue" label="pixel size">0.3</parameter>
    <!-- <parameter type="float" name="InnerVignetting" label="pixel size">0.7</parameter> -->
    <!-- <parameter type="float" name="OuterVignetting" label="pixel size">0.9</parameter> -->
    <parameter type="float" name="RandomValue" label="pixel size">0.0</parameter>
    <parameter type="float" name="TimeLapse" label="pixel size">0.5</parameter>
</filter>



